{
    "version": 2,
    "functions": {
        "dist/index.js": {
            "maxDuration": 30
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/dist/index.js"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/dist/index.js"
        }
    ]
}
