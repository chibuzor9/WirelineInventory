{
    "version": 2,
    "functions": {
        "dist/index.js": {
            "maxDuration": 30
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/dist/index.js"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/dist/public/assets/$1"
        },
        {
            "src": "/favicon.ico",
            "dest": "/dist/public/favicon.ico"
        },
        {
            "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg))",
            "dest": "/dist/public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/dist/public/index.html"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/dist/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}
